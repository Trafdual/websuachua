        <script>
                let selectedRating = 0;

                document.querySelectorAll('.star').forEach(star => {
                    star.addEventListener('click', () => {
                        const value = parseInt(star.getAttribute('data-value'));
                        if (value === selectedRating + 1) {
                            selectedRating = value;
                        } else {
                            selectedRating = value - 1; // Xóa chọn sao nếu nhấp vào sao đã chọn trước đó
                        }
                        updateRating();
                    });
                });

                function updateRating() {
                    document.querySelectorAll('.star').forEach((star, index) => {
                        if (index < selectedRating) {
                            star.style.color = '#ff0'; // Yellow color for selected stars
                        } else {
                            star.style.color = '#ccc'; // Gray color for unselected stars
                        }
                    });
                    document.getElementById('ratingInput').value = selectedRating; // Update hidden input value
                }
            </script>
