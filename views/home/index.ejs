        <script>
                document.addEventListener("DOMContentLoaded", function() {
                    const fanpageButton = document.getElementById("fanpageButton");
                    var leftImage = document.getElementById("left");
                    var rightImage = document.getElementById("right");
                    var footer = document.getElementById("footer");

                    fanpageButton.addEventListener("click", function() {
                        // Thay thế "URL_FANPAGE_FACEBOOK_CUA_BAN" bằng đường dẫn fanpage của bạn
                        window.location.href = "https://www.messenger.com/t/110013628554676/";
                    });

                    leftImage.style.transition = "bottom 0.3s ease-in-out";
                    rightImage.style.transition = "bottom 0.3s ease-in-out";

                    window.addEventListener("scroll", function() {
                        var footerPosition = footer.getBoundingClientRect();
                        var imagePosition = leftImage.getBoundingClientRect();

                        // Kiểm tra nếu hình ảnh gần phần footer
                        if (imagePosition.bottom >= footerPosition.top) {

                            leftImage.style.bottom = "60%";
                            rightImage.style.bottom = "60%";
                        } else {
                            // Nếu không, đặt lại vị trí của hình ảnh
                            leftImage.style.bottom = "35%";
                            rightImage.style.bottom = "35%";
                        }
                    });
                });
                document.addEventListener('DOMContentLoaded', function() {
                    const searchInput = document.getElementById('search-input');

                    searchInput.addEventListener('input', function() {
                        if (searchInput.value.length > 0) {
                            searchInput.classList.add('has-content');
                        } else {
                            searchInput.classList.remove('has-content');
                        }
                    });

                    searchInput.addEventListener('click', function(event) {
                        if (event.target === searchInput) {
                            const clearIcon = getComputedStyle(searchInput, '::after').content;
                            if (clearIcon === '"×"') {
                                searchInput.value = '';
                                searchInput.dispatchEvent(new Event('input')); // Để cập nhật hiển thị dấu x
                                searchInput.focus(); // Đặt lại tiêu điểm cho ô nhập liệu
                            }
                        }
                    });
                });
            </script>
        <script>
                function makeCall() {
                    event.preventDefault();
                    // Thay đổi số điện thoại dưới đây bằng số điện thoại thực tế
                    var phoneNumber = "0816354354";

                    // Kiểm tra xem trình duyệt có hỗ trợ gọi điện không
                    if ("contacts" in navigator && "ContactsManager" in window) {
                        // Sử dụng API Contacts để gọi điện
                        navigator.contacts.select(["phone"]).then(contacts => {
                            // Gọi điện đến số điện thoại đã được chỉ định
                            contacts.forEach(contact => {
                                contact.phoneNumbers.forEach(phone => {
                                    if (phone.value === phoneNumber) {
                                        contact.pick();
                                    }
                                });
                            });
                        });
                    } else {
                        // Mở giao diện gọi điện của trình duyệt
                        window.location.href = "tel:" + phoneNumber;
                    }
                }
                document.addEventListener("DOMContentLoaded", function() {
                    const callA = document.getElementById("callA");

                    if (callA) {
                        callA.addEventListener("click", makeCall);
                    }
                });
            </script>
        <script>
                function encodeSpecialCharacters(name) {
                    // Thay thế các kí tự đặc biệt bằng dấu -
                    const replacedName = name.replace(/[^\w\s]/gi, '-');
                    // Mã hóa tên sản phẩm và trả về
                    return encodeURIComponent(replacedName);
                }
